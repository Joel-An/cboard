<table class="table table-hover table-striped">
    <thead>
        <tr class="text-center"><strong>내가 쓴 글</strong></tr>
        <tr>
            <th class="text-center">게시판</th>
            <th>제목</th>
            <th class="text-center">날짜</th>
            <th class="text-center">추천수</th>
        </tr>
    </thead>
    <tbody>
        <% if(posts.length > 0) { %>
            <% posts.forEach(function(item) { %>
                <tr>
                    <td class="text-center"><%=item.boardInfo.nameKor%></td>
                    <td class="text-left"><a href="/board/<%=item.boardInfo.nameEng%>/<%=item._id%>"><%=item.title%> <small>[<%=item.commentCount%>]</small></a></td>
                    <td class="text-center text-muted" ><small><%=moment(item.date).format('MM/DD HH:mm')%></small></td>
                    <td class="text-center"><%=item.upVotes%></td>
                    </tr>
                <% }) %>
            <% } %>
    </tbody>
</table>